<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F7F8FA"
    android:orientation="vertical">

    <include layout="@layout/include_title_common9" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none">


        <LinearLayout
            android:id="@+id/ll_min3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ll_min2"
            android:layout_marginHorizontal="18dp"
            android:layout_marginTop="18dp"
            android:background="@drawable/white_all_bg1_1"
            android:orientation="vertical">

            <RelativeLayout
                android:id="@+id/rl_help1"
                android:layout_width="match_parent"
                android:layout_height="32dp"
                android:paddingHorizontal="8dp">


                <TextView
                    android:id="@+id/tv_box_time2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="1111"
                    android:textColor="@color/color_pic_text2"
                    android:textSize="14sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/icon_pic_min06" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#F2F3F5" />

            <RelativeLayout
                android:id="@+id/rl_help2"
                android:layout_width="match_parent"
                android:layout_height="32dp"
                android:paddingHorizontal="8dp">

                <TextView
                    android:id="@+id/tv_box_time34"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="2222"
                    android:textColor="@color/color_pic_text2"
                    android:textSize="14sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/icon_pic_min06" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#F2F3F5" />

            <RelativeLayout
                android:id="@+id/rl_help3"
                android:layout_width="match_parent"
                android:layout_height="32dp"
                android:paddingHorizontal="8dp">


                <TextView
                    android:id="@+id/tv_box_time23"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:text="33333"
                    android:textColor="@color/color_pic_text2"
                    android:textSize="14sp" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_centerVertical="true"
                    android:src="@drawable/icon_pic_min06" />
            </RelativeLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#F2F3F5" />


        </LinearLayout>


    </ScrollView>

</LinearLayout>